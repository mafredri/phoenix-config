{
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "Node",
		"baseUrl": "src",
		"outDir": "out/src",
		"target": "ES2021",
		"sourceMap": true,
		"strict": true,
		"noFallthroughCasesInSwitch": true,
		"pretty": true,
		"stripInternal": true,
		"types": ["phoenix"],
		"typeRoots": ["node_modules/@types"],
		"lib": ["ES2021"]
	},
	"exclude": ["node_modules"]
}
