{
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"baseUrl": "src",
		"target": "ES2024",
		"strict": true,
		"noEmit": true,
		"noFallthroughCasesInSwitch": true,
		"skipLibCheck": true,
		"types": ["phoenix"],
		"lib": ["ES2024"],
	},
	"include": ["src"],
	"exclude": ["node_modules", "out", "build.mjs"],
}
